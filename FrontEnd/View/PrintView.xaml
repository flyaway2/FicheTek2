                            <ColumnDefinition Width="4*" />
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="2*" />
                        </Grid.ColumnDefinitions>
                       
                        <TextBlock 
                          
                            Text="{Binding Comp6.GetMatiere.Titrage.TypeMatiere.MatiereNom}"
                            FontSize="16"
                        />
                       
